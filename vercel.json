{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "X-Deployment-Version",
          "value": "2.1-prompt-fix"
        }
      ]
    }
  ],
  "functions": {
    "app/page.tsx": {
      "maxDuration": 10
    }
  }
}